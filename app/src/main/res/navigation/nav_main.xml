<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_main"
    app:startDestination="@id/deviceListFragment">

    <fragment
        android:id="@+id/deviceListFragment"
        android:name="com.karry.managermyphone.DeviceListFragment"
        android:label="Device List"
        tools:layout="@layout/fragment_device_list" >
        <action
            android:id="@+id/action_deviceListFragment_to_deviceManagerFragment"
            app:destination="@id/deviceManagerFragment" />
    </fragment>
    <fragment
        android:id="@+id/deviceManagerFragment"
        android:name="com.karry.managermyphone.DeviceManagerFragment"
        android:label="Device Manager"
        tools:layout="@layout/fragment_device_manager" />
</navigation>